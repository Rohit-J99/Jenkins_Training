pipeline {
    agent any

    stages {
        stage('Build') {
            steps {
                echo 'Building..'
            }
        }
        stage('parallel stages') {
        parallel {
            stage('build') {
            steps {
                echo 'parallely building!'
            }
            }
            stage('test') {
            steps {
                echo 'parallely testing!'
            }
            }
        }
        }
  
        stage('Test') {
            steps {
                echo 'Testing.....'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying.....'
            }
        }
    }
}